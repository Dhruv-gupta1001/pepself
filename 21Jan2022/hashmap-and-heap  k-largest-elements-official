PriorityQueue<Integer> pq= new PriorityQueue<>();
      for(int x:arr){
          pq.add(x);
      }
      
      
      for(int i=0;i<n-k;i++){
          pq.remove();
      }
      for(int i=0;i<k;i++){
          System.out.println(pq.remove());
      }
    }
